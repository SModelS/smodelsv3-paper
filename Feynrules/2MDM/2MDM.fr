M$Parameters = {
  gZp == {
    ParameterType -> External,
    BlockName -> BLINPUTS,
    InteractionOrder -> {QED, 1},
    TeX              -> Subscript[g,Zp],
    Value -> 1.0,
    Description -> "U(1)Y B coupling coustant at the Zp pole"
  },
  MSd == {
    ParameterType -> External,
    BlockName -> BLINPUTS,
    Value -> 450.00,
    Description -> "Sd mass"
  },
  Sa == {
    ParameterType -> External,
    BlockName -> BLINPUTS,
    Value -> 0.1,
    Description -> "Sine of Higgses mixing angle"
  },
  gchi == {
    ParameterType -> External,
    InteractionOrder -> {NP, 1},
    BlockName -> ZPRIME,
    TeX              -> Subscript[g,chi],
    Value -> 1.0,
    Description -> "gauge coupling of Zprime with the DM"
  },   
  vev2 == {
    ParameterType    -> Internal,
    Value            -> MZp/(2*gZp*gchi),
    InteractionOrder -> {QED, -1},
    Description      -> "Sd VEV"
  },
  Ca == {
    ParameterType    -> Internal,
    Value            -> Sqrt[1-Sa^2],
    ParameterName    -> Ca,
    Description      -> "Cosine of Higgses mixing angle"
  },
  gqV== {
    ParameterType -> External,
    InteractionOrder -> {NP, 1},
    BlockName -> ZPRIME,
    TeX              -> Subscript[g,qV],
    Value -> 0.5,
    Description -> "Vector coupling of Zprime with the quarks"
  },
  gqA == {
    ParameterType -> External,
    InteractionOrder -> {NP, 1},
    BlockName -> ZPRIME,
    TeX              -> Subscript[g,qA],
    Value -> 0.1,
    Description -> "Axial coupling of Zprime with the quarks"
  },
  gqL == {
    ParameterType -> Internal,
    InteractionOrder -> {NP, 1},
    BlockName -> ZPRIME,
    TeX              -> Subscript[g,q],
    Value -> gqV - gqA,
    Description -> "Left coupling of Zprime with the quarks"
  },  
  gqR == {
    ParameterType    -> Internal,
    InteractionOrder -> {NP, 1},
    BlockName        -> ZPRIME,
    TeX              -> Subscript[g,qA],
    Value            -> gqV + gqA,
    Description      -> "Right coupling of Zprime with the quarks"
  }, 
  gSd == {
    ParameterType -> Internal,
    InteractionOrder -> {NP, 1},
    BlockName -> ZPRIME,
    TeX              -> Subscript[g,Sd],
    Value -> -2*gchi,
    Description -> "gauge coupling of Zprime with the leptons"
  },  
  ychi == {
    ParameterType -> External,
    InteractionOrder -> {NP, 1},
    Value   -> 1,
    InteractionOrder -> {QED, 1},
