  }, 
  (**************Higgs Potential*******************)
  lam1 == {
    ParameterType    -> Internal,
    Value            ->  (Mh^2 + MSd^2 +(Mh^2 - MSd^2)*(Ca^2 -Sa^2))/(4*vev^2),
